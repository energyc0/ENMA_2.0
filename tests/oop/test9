class Dog{
    field name;

    meth sound(){
        println("Bark!");
    }

    meth eat(food){
        println(name, " has eaten ", food, ". Yammy!");    
    }

    meth getname(){
        return name;
    }
}

func main(){
    var bro = Dog();
    bro.name = "Oliver";
    bro.sound();
    println("Dog's name is ", bro.getname());
    bro.eat("sandwich");
}