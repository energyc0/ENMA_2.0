class Dog{
    field name;
    field age;

    Dog(){
        name = "noname";
        age = 0;
    }
    Dog(name_){
        name = name_;
        age = 0;
    }
    Dog(name_, age_){
        name = name_;
        age = age_;
    }
}

func print_dog_info(dog){
    println(dog, " ", dog.name, " ", dog.age);
}

func main(){
    var a = Dog();
    print_dog_info(a);
    var b = Dog("Max");
    print_dog_info(b);
    var c = Dog("Alex", 5);
    print_dog_info(c);
}