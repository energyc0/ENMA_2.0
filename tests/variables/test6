var s = "Hello" + ", " + "world";
var a = (11 - 12) - 1 * 5 + ((4) / 2) * ((2) + 1);
var b = a + 1;
var c = true and a > 0;

var d = 11;

var e = d++;
var f = ++d;
var g = d--;
var h = --d;


func main(){
    print s + "!";
    print a;
    print b;
    print c;
    print d;
    print e;
    print f;
    print g;
    print h;
}