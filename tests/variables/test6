var s = "Hello" + ", " + "world";
var a = (11 - 12) - 1 * 5 + ((4) / 2) * ((2) + 1);
var b = a + 1;
var c = true and a > 0;

var d = 11;

var e = d++;
var f = ++d;
var g = d--;
var h = --d;


func main(){
    println(s + "!");
    println(a);
    println(b);
    println(c);
    println(d);
    println(e);
    println(f);
    println(g);
    println(h);
}