func main(){
    var a = "0 scope";
    {
        var a = "1 scope";
        {
            var a = "2 scope";
            {
                var a = "3 scope";
                println(a);
            }
            println(a);
        }
        println(a);
    }
    println(a);

    {
        var a = "4 scope";
        {
            var a = "5 scope";
            {
                var a = "6 scope";
                var somevar1 = 11;
                var somevar2 = 11;
                println(a);
            }
            println(a);
        }
        println(a);
    }
    println(a);
}