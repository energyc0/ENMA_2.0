func main(){
	println(+5 - 3);     		// 2
	println(-10 + +15);   		// 5
	println(+7 * -2);     		// -14
	println(-3 + +5 * -2);   	// -3 + (5 * -2) = -13
	println( +4 - -6 / +2);    	// 4 - (-6 / 2) = 7
	println( +(+(-(+(5)))));    // -5
	println( -(+2) * -(+3));    // (-2) * (-3) = 6
	println( +3 + -2 * +2);     // 3 + (-2 * 2) = -1
	println( -(2 + +3) * -4);   // -(5) * -4 = 20
	println( + + + 5);          // 5
	println( - - - 5);          // -5
	println( +0013);			//13
}