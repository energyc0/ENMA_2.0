func main(){
    var a = 0;
    for(; a < 5; a++){
        if(a > 2){
            print "break!";
            break;
        }
        print a;
    }

    while(true){
        print "sadflasdflsad";
        print "break!";
        break;
    }

    for(var i = 0; i < 3; i++){
        for(var j = 0; j < 3; j++){
            for(var k = 0; k < 3; k++){
                
                if(k < 1){print k + i + j;} else { print "break!";break;}
            }
            if(j > 1){
                print "break!";
                break;
            }
        }
    }


    print "end.";
}