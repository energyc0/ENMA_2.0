func main(){
    var a = 0;
    for(; a < 5; a++){
        if(a > 2){
            println("break!");
            break;
        }
        println(a);
    }

    while(true){
        println("sadflasdflsad");
        println("break!");
        break;
    }

    for(var i = 0; i < 3; i++){
        for(var j = 0; j < 3; j++){
            for(var k = 0; k < 3; k++){
                
                if(k < 1){println(k + i + j);} else { println("break!");break;}
            }
            if(j > 1){
                println("break!");
                break;
            }
        }
    }


    println("end.");
}