func sum(a,b);
func mid(a,b,c);
func my_print(val);

func sum(a, b){
	return a + b;
}

func mid(a,b,c){
	return (a + b + c) / 3;
}

func my_print(val){
	println(val);
}

var someval = 123;

func main(){

	println(sum(1,2));
	println(mid(1,2,3));
	println(mid(3,3,6));
	println(sum(10,16));
	my_print(true);
	my_print(111);
	my_print("hello!");

	var temp = -23;
	println(sum(someval, temp));
	my_print(temp + someval);
	println(sum(1+2,3+4/2));
}