func main(){
    println("start.");

    for(var i = 0; i < 3; i++){
        println(i);
        continue;
        println("bad");
    }

    var a = 0;
    while(a > -3){
        
        for(var i = 0; i < 5; i++){
            for(var j = 0; j < 10; j++){
                continue;
                println("bad");
            }
            continue;
            println("bad");
        }
        println(a--);
        continue;
        println("bad");
    }


    println("end.");
}